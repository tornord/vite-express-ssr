{
  "name": "@vite-express-ssr/marko",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@marko/express": "^2.1.0",
    "compression": "^1.7.4",
    "exp-config": "^4.2.0",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "marko": "^5.22.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@marko/compiler": "^5.23.4",
    "@marko/vite": "^2.3.12",
    "vite": "^4.1.1"
  },
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production vite build --ssr app.js",
    "dev": "mkdir -p dist && node --enable-source-maps --experimental-specifier-resolution=node app.js",
    "dev:inspect": "node --inspect --enable-source-maps app.js",
    "start": "NODE_ENV=production node --enable-source-maps --experimental-specifier-resolution=node dist/app.js"
  }
}
